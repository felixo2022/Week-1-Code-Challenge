function Grade(){
const prompt = require("prompt-sync")()
let marks = prompt("Enter your marks:")
if (marks >= 79 && marks <= 100) {
    return A;
}
else if (marks >= 60 && marks <= 78) {
    return B
}
else if (marks >= 49 && marks <= 59) {
    return C
}
else if (marks >= 40 && marks <= 48) {
    return D
}

else {
    return E
}
}
function Grade();